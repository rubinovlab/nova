datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Gene {
  id            Int    @id @default(autoincrement())
  geneId        String
  chromosome    String
  startPosition Int
  endPosition   Int
  pValue        Float
  beta          Float
  geneSymbol    String
  grex          String
  phenotype     String
  pBonferroni   Float
  pFDR          Float
}

model Grex {
  id     Int     @id @default(autoincrement())
  geneId String
  grex   String
  grexes Float[]
}

model Volume {
  id        Int     @id @default(autoincrement())
  phenotype String
  volumes   Float[]
}

model Gene2 {
  id            Int    @id @default(autoincrement())
  geneId        String
  chromosome    String
  startPosition Int
  endPosition   Int
  pValue        Float
  beta          Float
  geneSymbol    String
  grex          String
  phenotype     String
  pBonferroni   Float
  pFDR          Float
}

model Grex2 {
  id     Int     @id @default(autoincrement())
  geneId String
  grex   String
  grexes Float[]
}

model GenePosition {
  id         Int    @id @default(autoincrement())
  chromosome String
  start      Int
  end        Int
  geneID     String
}
